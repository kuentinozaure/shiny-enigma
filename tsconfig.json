{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "types": [
      "node"
   ],
   "typeRoots": [
      "../node_modules/@types"
   ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "dbviewer": [
        "dist/dbviewer"
      ],
      "dbviewer/*": [
        "dist/dbviewer/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
